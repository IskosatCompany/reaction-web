    "@types/file-saver": "^2.0.7",
    "@vitest/ui": "^4.0.15",
    "angular-eslint": "20.2.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2",
